  <script src="{{ asset('assets/vendor/php-email-form/validate.js')}}"></script>
<script>function myFunction() {
  var x = document.getElementById("password");
  if (x.type === "password") {
    x.type = "text";
  } else {
    x.type = "password";
  }
}</script>
  <!-- Template Main JS File -->
  <script src="{{ asset('assets/js/main.js')}}"></script>
        {% endblock %}
